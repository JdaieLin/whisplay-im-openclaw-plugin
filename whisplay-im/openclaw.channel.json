{
  "name": "whisplay-im",
  "displayName": "Whisplay IM",
  "description": "HTTP bridge channel for Whisplay IM chatbot",
  "config": [
    {
      "key": "ip",
      "label": "Bridge IP/Host",
      "type": "string",
      "required": true,
      "placeholder": "192.168.1.100:18888"
    },
    {
      "key": "token",
      "label": "Token (Optional)",
      "type": "string",
      "required": false,
      "secret": true,
      "placeholder": "your bearer token"
    },
    {
      "key": "waitSec",
      "label": "Poll waitSec",
      "type": "number",
      "required": false,
      "default": 30
    }
  ]
}
